<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        <title>{% block title %}Milestone!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Nunito+Sans:200,300,400,700,900|Roboto+Mono:300,400,500"> 
            <link rel="stylesheet" href="{{ asset('assets/milestone/fonts/icomoon/style.css') }}">

            <link rel="stylesheet" href="{{ asset('assets/milestone/css/bootstrap.min.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/css/magnific-popup.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/css/jquery-ui.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/css/owl.carousel.min.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/css/owl.theme.default.min.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/css/bootstrap-datepicker.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/css/mediaelementplayer.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/css/animate.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/fonts/flaticon/font/flaticon.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/css/fl-bigmug-line.css') }}">

            <link rel="stylesheet" href="{{ asset('assets/milestone/css/aos.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/milestone/css/style.css') }}">
        {% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
    {% block body %}
    <div class="site-loader"></div>
    <div class="site-wrap">
        <div class="site-mobile-menu">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
        <div class="site-mobile-menu-body"></div>
        </div> <!-- .site-mobile-menu -->
        
        <!-- See Properties Section Begin -->
        {% include "milestone/see.properties.html.twig" %}
        <!-- See Properties Section End -->
        
        <!-- Footer Section Begin -->
        {% include "milestone/footer.html.twig" %}
        <!-- Footer Section End -->
    </div>
    {% endblock %}

        {% block javascripts %}
            <script src="{{ asset('assets/milestone/js/jquery-3.3.1.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/jquery-migrate-3.0.1.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/jquery-ui.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/popper.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/owl.carousel.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/mediaelement-and-player.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/jquery.stellar.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/jquery.countdown.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/jquery.magnific-popup.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/bootstrap-datepicker.min.js') }}"></script>
            <script src="{{ asset('assets/milestone/js/aos.js') }}"></script>

            <script src="{{ asset('assets/milestone/js/main.js') }}"></script>
        {% endblock %}
    </body>
</html>
